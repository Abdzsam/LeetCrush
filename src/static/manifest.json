
{
  "manifest_version": 3,
  "name": "LeetCrush",
  "description": "Start Crushing!",
  "version": "1.0.0",
  "action" : {
      "default_popup": "popup.html",
      "default_title": "LeetCrush",
      "default_icon" : "icon.webp"
  },
  "chrome_url_overrides": {
      "newtab": "newTab.html"
  },
  "permissions": [
      "tabs",
      "bookmarks",
      "storage",
      "identity"
  ],
  "icons": {
      "16" : "icon.webp",
      "48" : "icon.webp",
      "128" : "icon.webp"
  },
  "options_page": "options.html",
  "background": {
      "service_worker": "background.js"
  },
  "content_scripts": [
      {
          "matches": ["<all_urls>"],
          "js": ["contentScript.js"]
      }
  ]
}
